{
	"name": "Ethiopian Monitor",
	"showCase": {
		"url": "https://ethiopianmonitor.com/category/business/",
		"selectorObjs": [
			{
				"name": "links",
				"selector": "div.article-container article a.more-link",
				"type": "array",
				"attribute": "href",
				"outputType": "array",
				"webDomain": "https://ethiopianmonitor.com"
			}
		]
	},
	"news": {
		"selectorObjs": [
			{
				"name": "title",
				"selector": "div.article-content .entry-title"
			},
			{
				"name": "article",
				"selector": "#primary div.article-content .entry-content p:not(p:last-child)",
				"type": "array",
				"outputType": "string",
				"delimiter": "$$$\n"
			},
			{
				"name": "date",
				"selector": ".article-content time",
				"attribute": "dateTime"
			},
			{
				"name": "image",
				"selector": "img.attachment-colormag-featured-image",
				"attribute": "src",
				"webDomain": "https://ethiopianmonitor.com"
			},
			{
				"name": "author",
				"selector": ".url.fn.n"
			}
		],
		"savingLocation": "src/scraper/data/out/ethioMonitor.json"
	}
}
