{
	"showCase": {
		"url": "https://www.ena.et/?cat=31",
		"selectorObjs": [
			{
				"name": "links",
				"selector": ".td-ss-main-content h3.entry-title a",
				"type": "array",
				"attribute": "href",
				"outputType": "array",
				"webDomain": "https://ena.com"
			}
		]
	},
	"news": {
		"selectorObjs": [
			{
				"name": "title",
				"selector": ".td-post-header .td-post-title .entry-title"
			},
			{
				"name": "article",
				"selector": ".td-post-content p",
				"type": "array",
				"outputType": "string",
				"delimiter": "$$$\n"
			},
			{
				"name": "date",
				"selector": "footer meta:nth-child(7)",
				"attribute": "content"
			},
			{
				"name": "image",
				"selector": ".td-post-content .td-post-featured-image a",
				"attribute": "href",
				"webDomain": "https://ena.com"
			},
			{
				"name": "author",
				"selector": "",
				"default": "ENA"
			}
		],
		"savingLocation": "data/out/ena.json"
	}
}
